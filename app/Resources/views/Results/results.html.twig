<div id="map" class="col-sm-offset-2 col-sm-8"></div>

<script type="text/javascript">

    var map;
    var myLatLng = {lat: 46.041, lng: 6.155};
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 46.255, lng: 6.644},
            zoom: 8
        });


        map.data.loadGeoJson('https://raw.githubusercontent.com/Noa83/Activit-_Git/master/carroyage10km.json');
//            var KmlLayer = new google.maps.KmlLayer({
//                url: 'https://inpn.mnhn.fr/docs/KMZ/L93_10K.kmz',
//                map: map
//            });
        map.data.setStyle({
            fillColor: 'green',
            strokeWeight: 1
        });

        var marker = new google.maps.Marker({
            position: myLatLng,
            map: map,
            title: 'Villy!'
        });
    }



</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDHtk1Q7isnHjGK7ApFU2_V5--RdvqVUks&callback=initMap">
